{
	"_version": "1.4.0",
	"sap.app": {
		"id": "carrpasp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/CARRPASP/carrpasp.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": [
						"localAnnotations",
						"annotation01"
					]
				}
			},
			"localAnnotations": {
				"uri": "localService/metadata.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			},
			"annotation01": {
				"uri": "annotations/annotation0.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "annotations/annotation0.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.2masterdetail",
			"version": "1.44.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "carrpasp.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.44.12",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.44.12"
				},
				"sap.m": {
					"minVersion": "1.44.12"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "carrpasp.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"defaultBindingMode": "TwoWay",
					"metadataUrlParams": {
						"sap-documentation": "heading"
					}
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "carrpasp.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"menu",
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "menu",
					"target": [
						"menu"
					]
				},
				{
					"pattern": "Segmentapp",
					"name": "Segmentapp",
					"target": [
						"Segmentapp"
					]
				},
				{
					"pattern": "segmentTbl",
					"name": "segmentTbl",
					"target": [
						"segmentTbl",
						"segvalTbl"
					]
				},
				{
					"pattern": "SEGMENT/{objectId}",
					"name": "segvalTbl",
					"target": [
						"segmentTbl",
						"segvalTbl"
					]
				},
				{
					"pattern": "cpaspTbl",
					"name": "cpaspTbl",
					"target": [
						"cpaspTbl"
					]
				},
				{
					"pattern": "CPASP/{objectId}",
					"name": "cpaspView",
					"target": [
						"cpaspView"
					]
				},
				{
					"pattern": "CPASP_U/{objectId}",
					"name": "cpaspEdit",
					"target": [
						"cpaspEdit"
					]
				},
				{
					"pattern": "cpaspChart",
					"name": "cpaspChart",
					"target": [
						"cpaspChart"
					]
				}
			],
			"targets": {
				"menu": {
					"viewName": "Menu",
					"viewId": "menu",
					"viewLevel": 1,
					"title": "Меню"
				},
				"Segmentapp": {
					"viewName": "Segmentapp",
					"viewLevel": 1,
					"viewId": "Segmentapp"
				},
				"segmentTbl": {
					"viewName": "SegmentTbl",
					"viewLevel": 2,
					"viewId": "segmentTbl",
					"controlAggregation": "masterPages"
				},
				"segvalTbl": {
					"viewName": "SegvalTbl",
					"viewId": "segvalTbl",
					"viewLevel": 3,
					"controlAggregation": "detailPages"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				},
				"cpaspTbl": {
					"viewName": "CPaspTbl",
					"viewId": "CPaspTbl",
					"viewLevel": 1
				},
				"cpaspView": {
					"viewName": "CPaspView",
					"viewId": "CPaspView",
					"viewLevel": 2
				},
				"cpaspEdit": {
					"viewName": "CPaspEdit",
					"viewId": "CPaspEdit",
					"viewLevel": 3
				},
				"repairTbl": {
					"viewName": "RepairTbl",
					"viewId": "RepairTbl",
					"viewLevel": 3
				},
				"crunTbl": {
					"viewName": "CrunTbl",
					"viewId": "CrunTbl",
					"viewLevel": 3
				},
				"cjurTbl": {
					"viewName": "CjurTbl",
					"viewId": "CjurTbl",
					"viewLevel": 3
				},
				"cpaspChart": {
					"viewName": "CPaspChart",
					"viewId": "CPaspChart",
					"viewLevel": 2
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}